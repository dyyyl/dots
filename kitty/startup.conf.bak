# Setup initial work tab state
new_tab work

# Set initial tab layout
layout tall

# Open main working window
launch --title "main" --cwd=current

# Setup initial play tab state
new_tab play

# Set initial tab layout
layout fat

# Open initial windows in home directory
cd ~

launch --cwd=current --title "play" spotify_player
launch --cwd=current --title "monitor" btm --rate 1500 --default_time_value 30000
